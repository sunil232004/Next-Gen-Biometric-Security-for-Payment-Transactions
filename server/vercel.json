{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**/*.ts", "**/*.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/index.ts"
    },
    {
      "src": "/favicon.png", 
      "dest": "/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.ts"
    }
  ]
}
